package com.example.pg.ds_examples.src.arrays;

public class ReverseNumber {

	public static void main(String[] args) {
		int number=345;
		int rem=0;
		while(number > 0) {
			if(rem == 0)
				rem += number % 10;
			else
				rem = rem * 10 + number % 10;
			number /= number;
			
			
		}
		System.out.println(rem);

	}

}
